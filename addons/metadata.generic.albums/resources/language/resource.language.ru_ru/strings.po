msgctxt "#30000"
msgid "Preferences"
msgstr "Предпочтения"

msgctxt "#30001"
msgid "Preferred language for album review"
msgstr "Предпочитаемый язык для обзора альбома"

msgctxt "#30002"
msgid "Prefer genres from"
msgstr "Предпочитаемые жанры с"

msgctxt "#30003"
